<Page
    x:Class="BassBooster.OneDriveSyncPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:BassBooster"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="#FF2C2C2C">
        <Button x:Name="LiveLoginButton" VerticalAlignment="Top" Margin="30,30,0,0" Content="Login to OneDrive" Click="LiveLoginButton_Click" />
        <TextBlock x:Name="StatusBlock" HorizontalAlignment="Left" Margin="300,30,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Text="Aaa" Style="{StaticResource BodyTextBlockStyle}"/>
        <Button x:Name="bb" Content="Button" HorizontalAlignment="Left" Margin="72,110,0,0" VerticalAlignment="Top" Click="bb_Click"/>
        <Button Content="Delete" HorizontalAlignment="Left" Margin="245,133,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <Button x:Name="Upload" Content="Upload" HorizontalAlignment="Left" Margin="423,113,0,0" VerticalAlignment="Top" Click="Upload_Click"/>
        <Button x:Name="CancelUpload" Content="Cancel" HorizontalAlignment="Left" Margin="574,79,0,0" VerticalAlignment="Top" Click="CancelUpload_Click"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="881,184,0,0" VerticalAlignment="Top" Click="Button_Click_1"/>
    </Grid>
</Page>
